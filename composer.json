{
    "name": "zfcampus/zf-http-cache",
    "description": "ZF2 Module providing http cache headers support",
    "license": "BSD-3-Clause",
    "keywords": [
        "zendframework",
        "api",
        "apigility",
        "zf",
        "zend",
        "module",
        "cache"
    ],
    "support": {
        "issues": "https://github.com/zfcampus/zf-http-cache/issues",
        "source": "https://github.com/zfcampus/zf-http-cache",
        "rss": "https://github.com/zfcampus/zf-http-cache/releases.atom",
        "chat": "https://zendframework-slack.herokuapp.com",
        "forum": "https://discourse.zendframework.com/c/questions/apigility"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-eventmanager": "^2.6.3 || ^3.0.1",
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-mvc": "^2.7.9 || ^3.0.2"
    },
    "require-dev": {
        "container-interop/container-interop": "^1.1",
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "ZF\\HttpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\HttpCache\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev",
            "dev-develop": "1.4.x-dev"
        },
        "zf": {
            "module": "ZF\\HttpCache"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    },
    "homepage": "http://apigility.org/",
    "authors": [
        {
            "homepage": "https://github.com/corentin-larose/",
            "name": "Corentin Larose",
            "role": "Developer"
        }
    ]
}
